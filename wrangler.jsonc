      "remote": true,
    },
    {
      "binding": "HANDLE_CACHE",
      "id": "f2ef5b0c21094c3582dc837c5dd7dc16",
      "remote": true,
    },
    {
      "binding": "OAUTH_STATE_STORE",
      "id": "4ad9cfcdb73f41e8a833b530ec656d30",
      "remote": true,
    },
    {
      "binding": "OAUTH_SESSION_STORE",
      "id": "db69a9eec5734159866620ea15a31a47",
      "remote": true,
    },
  ],
  // Environment variables
  "vars": {
